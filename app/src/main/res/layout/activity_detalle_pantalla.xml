<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.DetallePantallaActivity">


    <EditText
        android:id="@+id/inputNombre"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="53dp"
        android:background="@drawable/edittextbackground"

        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateIds" />

    <EditText

        android:id="@+id/inputPulgadas"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/edittextbackground"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/inputEstado" />

    <EditText

        android:id="@+id/inputLocalizacion"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/edittextbackground"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/inputEstado" />

    <EditText

        android:id="@+id/inputEstado"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/edittextbackground"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/inputApellido2" />

    <EditText

        android:id="@+id/inputApellido2"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/edittextbackground"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/inputApellido1"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/inputApellido1"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/edittextbackground"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/inputCorreo" />

    <EditText
        android:id="@+id/inputCorreo"
        android:layout_width="197dp"
        android:layout_height="27dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/edittextbackground"
        android:ems="10"

        android:inputType="textPersonName"
        android:paddingStart="5dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/inputNombre"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="53dp"
        android:text="N Serie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="19dp"
        android:text="Categoria"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="19dp"
        android:text="Marca"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="19dp"
        android:text="Modelo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="19dp"
        android:text="Estado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="19dp"
        android:text="Localizacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="176dp"
        android:layout_marginTop="340dp"
        android:text="Caracteristicas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="80dp"
        android:layout_height="27dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="70dp"
        android:text="Pulgadas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />
    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/azulComercio"
        android:backgroundTint="@color/azulComercio"
        android:gravity="end"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/botonVolver"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/botonAccionCentral"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="144dp"
                android:backgroundTint="@color/white"
                android:text="Modificar"
                android:textColor="@color/black"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/botonAccionDetalles"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:backgroundTint="@color/white"
                android:text="Reservar"
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/botonVolverDetalles"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:backgroundTint="@color/white"
                android:text="Volver"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>